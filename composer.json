{
    "name": "bolechen/gongmall-php-sdk",
    "description": "工猫（gongmall.com）sdk for php，非官方支持。基于官网文档：https://opendoc.gongmall.com 实现",
    "license": "MIT",
    "authors": [{
        "name": "Bole Chen",
        "email": "avenger@php.net"
    }],
    "require": {
        "php": "^7.2",
        "hanson/foundation-sdk": "3.0.*|^4.0.3|^5.0|dev-master",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "Bolechen\\Gongmall\\": "src",
            "Bolechen\\Gongmall\\Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
